{
  "name": "calendar",
  "version": "0.1.0",
  "description": "calendar",
  "main": "server.js",
  "author": "Burdin Bogdan",
  "scripts": {
    "start": "better-npm-run start:development",
    "start:production": "better-npm-run start:production",
    "build": "backpack build",
    "#postinstall": "backpack build",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest",
    "test:w": "jest --watch",
    "test:c": "jest --coverage",
    "precommit": "lint-staged"
  },
  "betterScripts": {
    "start:development": {
      "command": "backpack",
      "env": {
        "NODE_ENV": "development",
        "DB_STRING_CONNECTION":"mongodb://localhost/calendarDB",
        "SECRET_TOKEN_WORD": "secretword",
        "PORT": 8080
      }
    },
    "start:production": {
      "command": "node src/index.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "body-parser": "1.4.3",
    "express": "4.16.2",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "8.1.0",
    "method-override": "2.1.3",
    "mongoose": "4.4.12",
    "morgan": "1.1.1",
    "node-geocoder": "3.21.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "8.0.2",
    "babel-preset-es2015": "^6.24.1",
    "backpack-core": "0.4.3",
    "better-npm-run": "0.1.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "husky": "^0.14.3",
    "jest": "21.2.1",
    "lint-staged": "^4.3.0",
    "prettier": "^1.8.2",
    "supertest": "3.0.0"
  },
  "engines": {
    "node": "8.7.0",
    "npm": "5.4.2",
    "yarn": "1.3.2"
  }
}
